      "type": "image-with-text-type2",
      "blocks": {
        "template--16931313156397__fa50d998-28ac-4dc9-8fc2-125d5cc72884-16684986209e18587c-0": {
          "type": "image",
          "settings": {
            "content_type": "image",
            "image": "shopify:\/\/shop_images\/PSI.jpg",
